{
    "name": "highlight-words",
    "displayName": "highlight-words",
    "description": "Highlight all occurrences of words or expressions",
    "version": "0.0.5",
    "icon": "images/highlight.png",
    "publisher": "rsbondi",
    "engines": {
        "vscode": "^1.10.0"
    },
	"repository": {
		"type": "git",
		"url": "https://github.com/rsbondi/highlight-words"
	},
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.addHighlight",
                "title": "Highlight Selection"
            },
            {
                "command": "extension.removeHighlight",
                "title": "Highlight Remove"
            },
            {
                "command": "extension.addRegExpHighlight",
                "title": "Highlight Expression"
            },
            {
                "command": "extension.addHighlightWithOptions",
                "title": "Highlight Selection with Options"
            }
        ]
    }
}